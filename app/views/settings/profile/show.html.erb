<% content_for :style do %>
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <% SellerProfile::FONT_CHOICES.each do |font| %>
    <% if font != "ABC Favorit" %>
      <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=<%= font %>:wght@400;600&display=swap">
    <% end %>
  <% end %>
<% end %>
<%= load_pack("user") %>
<%= react_component "ProfileSettingsPage", props: @react_component_props, prerender: true %>
